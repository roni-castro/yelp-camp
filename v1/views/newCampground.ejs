<% include partials/header.ejs %>
    <div class="container justify-content-center">
        <form method="POST" action="/campgrounds" class="form-horizontal">
           <div class="form-row">
                <div class="form-group">
                     <label for="name">Camp name:</label>
                    <input name="name" class="form-control" type="text" placeholder="Type the camp name"></input>
                </div>
            </div>
            <div class="form-row">
                <div class="form-group">
                    <label for="photo">Photo url:</label>
                    <input name="photo" class="form-control" type="text" placeholder="Type the photo url"></input>
                </div>
           </div>
           <div class="text-center">
               <button type="submit" class="btn btn-primary">Create</button>
               <a href="/campgrounds" class="btn btn-primary">Cancel</a>
           </div>
        </form>
    </div>

<% include partials/footer.ejs %>